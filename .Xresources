! Xorg cursor
Xcursor*theme            : Neutral_Plus
! XTerm Settings
!xterm*termName           : xterm-256color
!XTerm*locale             : true
!XTerm*metaSendsEscape    : true
!XTerm*saveLines          : 4096 
!xterm*bellIsUrgent       : true
!xterm*highlightSelection : true
!XTerm*cursorUnderLine    : true
!XTerm*jumpScroll         : true
!XTerm*multiScroll        : true
!XTerm*fastScroll         : true
!XTerm*cutNewline         : false

! XTerm Fonts
!XTerm*faceName: Tamsyn:pixelsize=14:antialias=false
!xterm*faceNameDoublesize: xft:WenQuanYi Bitmap Song:size=8:antialias=false

! Dpi settings
Xft.dpi: 96.0

! Misc settings
URxvt*cursorBlink       : true
!URxvt*cursorUnderline   : true
URxvt*intensityStyles   : true
URxvt*jumpScroll        : true
URxvt*pointerBlank      : true
URxvt*saveLines         : 4096
URxvt*scrollBar         : false
URxvt*skipBuiltinGlyphs : false
URxvt*skipScroll        : true
URxvt*urgentOnBell      : true
URxvt*visualBell        : true
URxvt*internalBorder    : 0
URxvt*externalBorder    : 0

! Fonts
URxvt*font: \
    xft:Termsynu:pixelsize=14:antialias=false, \
    xft:Loli Gothic:pixelsize=10:antialias=false, \
    xft:WenQuanYi Bitmap Song:size=8:antialias=false, \
    xft:Fixed
URxvt*boldFont: \
    xft:Termsynu:pixelsize=14:antialias=false:style=bold, \
    xft:Loli Gothic:pixelsize=10:antialias=false, \
    xft:WenQuanYi Bitmap Song:size=8:antialias=false, \
    xft:Fixed

! Extensions
URxvt*perl-ext-common : default,matcher2
URxvt*matcher*button : 1
URxvt*url-launcher : firefox

#include "Molokai.xrdb"
!#include "solarized.xrdb"
